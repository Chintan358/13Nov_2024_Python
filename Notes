Djano : 

create env : 
py -m venv <envname>

<envname>/Scripts/activate

install django : 
pip install django

create project : 
py -m django startproject <projecname>

create app : 
switch to project
py manage.py stattapp <appname>

runserver : 
py manage.py runserver

setup HTML : 

setting.py => Templates => Dir {BASE_DIR/"folder name"}

Create model : 
py manage.py Makemigrations
py manage.py Migrate
py manage.py  createsuperuser
