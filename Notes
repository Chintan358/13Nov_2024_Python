# Djano : 

# create env : 
# py -m venv <envname>

# <envname>/Scripts/activate

# install django : 
# pip install django

# create project : 
# py -m django startproject <projecname>

# create app : 
# switch to project
# py manage.py stattapp <appname>

# runserver : 
# py manage.py runserver

# setup HTML : 

# setting.py => Templates => Dir {BASE_DIR/"folder name"}

# Create model : 
# py manage.py Makemigrations
# py manage.py Migrate
# py manage.py  createsuperuser


# Django Model query : 
# Create model
# import model
# Ex : Student [username,email,phone,age]
# save data : 

# s = Student(username="Test",email="test@gmail.com",age=25,phone="8989898989")
# s.save()

# Student.objects.create(username="")

# view Data : 
# Student.objects.all()
# Stduent.objects.get(id=1)

